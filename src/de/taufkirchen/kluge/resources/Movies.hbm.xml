<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.taufkirchen.kluge.MovieDB.impl.MoviesImpl" table="MOVIES">
        <id name="ID" type="java.lang.Integer">
            <column name="ID" />
        </id>
        <property name="Title" type="java.lang.String">
            <column name="TITLE" />
        </property>
        <property name="ThreeD" type="java.lang.Boolean">
            <column name="THREED" />
        </property>
    </class>
</hibernate-mapping>
